<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:hardwareAccelerated="true">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application

        android:name=".AroundConfig"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/finalsuuu"
        android:label="@string/app_name"
        android:roundIcon="@drawable/finalsuuu"
        android:supportsRtl="true"
        android:theme="@style/Theme.SuperWheel"
        tools:targetApi="31">

        <activity
            android:name=".Policy_View"
            android:exported="false" />

        <activity
            android:name=".Web_View"
            android:exported="false" />
        <activity
            android:name=".AlertConfig"
            android:exported="false" />
        <activity
            android:name=".FrontConfig"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>